{
	"name": "shadon/framework",
	"description": "shadon php framework",
	"keywords": [
		"api",
		"phalcon",
		"modules",
		"swoole"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Shadon",
			"homepage": "http://www.shadon.org"
		}
	],
	"require": {
		"php": "^7.3.10",
		"ext-apcu":"^5.1.17",
		"ext-igbinary":"^3.0.1",
		"ext-mongodb":"^1.6.0",
		"ext-phalcon":"^3.4.4",
		"ext-swoole":"^4.4.8",
		"guzzlehttp/guzzle": "^6.2.3",
		"mongodb/mongodb": "^1.5.0",
		"monolog/monolog": "^2.0.0",
		"php-amqplib/php-amqplib": "^2.10.1",
		"predis/predis": "^1.1.1",
		"ralouphie/getallheaders": "^3.0.3",
		"symfony/console": "^4.3.5",
		"symfony/debug": "^4.3.5",
		"symfony/event-dispatcher": "^4.3.5",
		"vlucas/phpdotenv": "^3.6.0"
	},
	"autoload": {
		"psr-4": {
			"Shadon\\": "src/Shadon/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	},
	"config": {
		"sort-packages": true
	}
}
