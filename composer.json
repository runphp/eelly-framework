{
	"name": "shadon/framework",
	"description": "shadon php framework",
	"keywords": [
		"api",
		"modules",
		"swoole"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Shadon",
			"homepage": "http://www.shadon.org"
		}
	],
	"require": {
		"php": "^7.3",
		"ext-apcu":"^5.1",
		"ext-igbinary":"^3.0",
		"ext-mongodb":"^1.6",
		"ext-swoole":"^4.4",
		"defuse/php-encryption": "^v2.2",
		"doctrine/annotations": "^1.8",
		"doctrine/cache": "^1.8",
		"enqueue/amqp-lib": "^0.9",
		"enqueue/enqueue": "^0.9",
		"guzzlehttp/guzzle": "^6.2",
		"illuminate/config": "^6.3",
		"illuminate/database": "^6.3",
		"illuminate/events": "^6.3",
		"illuminate/pagination": "^6.3",
		"jenssegers/mongodb": "^3.6",
		"mongodb/mongodb": "^1.5",
		"monolog/monolog": "^2.0",
		"nikic/fast-route": "^1.3",
		"nikic/php-parser": "^4.3",
		"nunomaduro/collision": "^3.0",
		"ocramius/proxy-manager": "^2.2",
		"php-di/php-di": "^6.0",
		"phpdocumentor/reflection-docblock": "^4.3",
		"predis/predis": "^1.1",
		"symfony/cache": "^4.3",
		"symfony/console": "^4.3",
		"symfony/debug": "^4.3",
		"symfony/event-dispatcher": "^4.3",
		"symfony/finder": "^4.3.5",
		"vlucas/phpdotenv": "^3.6",
		"zendframework/zend-diactoros": "^2.2",
		"zendframework/zend-httphandlerrunner": "^1.1",
		"zendframework/zend-stratigility": "^3.2",
		"zendframework/zend-expressive-swoole": "^2.4"
	},
	"autoload": {
		"psr-4": {
			"Shadon\\": "src/Shadon/"
		},
		"files": [
			"src/Shadon/Helper/functions_include.php"
		]
	},
	"bin": [
		"bin/shadon"
	],
	"config": {
		"sort-packages": true
	}
}
